        let interval;
        if (isRunning) {
            interval = setInterval(()=>{
                if (seconds === 0) {
                    if (minutes === 0) {
                        handleTimerComplete();
                    } else {
                        setMinutes(minutes - 1);
                        setSeconds(59);
                    }
                } else {
                    setSeconds(seconds - 1);
                }
            }, 1000);
        }
        return ()=>clearInterval(interval);
    }, [
        isRunning,
        minutes,
        seconds
    ]);
    const calculateRank = (xp)=>{
        for(let i = RANKS.length - 1; i >= 0; i--){
            if (xp >= RANKS[i].minXP) {
                return RANKS[i];
            }
        }
        return RANKS[0];
    };
    const calculateLevel = (xp)=>{
        return Math.floor(xp / 100) + 1;
    };
    const handleTimerComplete = ()=>{
        setIsRunning(false);
        if (!isBreak) {
            const newXP = userData.xp + XP_PER_POMODORO;
            const newRank = calculateRank(newXP);
            const newLevel = calculateLevel(newXP);
            setUserData((prev)=>({
                    ...prev,
                    coins: prev.coins + COINS_PER_POMODORO,
                    totalCoins: prev.totalCoins + COINS_PER_POMODORO,
                    xp: newXP,
                    level: newLevel,
                    rank: newRank.name,
                    rankIcon: newRank.icon,
                    pomodorosCompleted: prev.pomodorosCompleted + 1
                }));
            showRewardNotification(`Pomodoro Complete! +${COINS_PER_POMODORO} coins, +${XP_PER_POMODORO} XP`);
            setIsBreak(true);
            setMinutes(BREAK_MINUTES);
            setSeconds(0);
        } else {
            setIsBreak(false);
            setMinutes(POMODORO_MINUTES);
            setSeconds(0);
        }
    };
    const showRewardNotification = (message)=>{
        setRewardMessage(message);
        setShowReward(true);
        setTimeout(()=>setShowReward(false), 3000);
    };
    const toggleTimer = ()=>{
        setIsRunning(!isRunning);
    };
    const resetTimer = ()=>{
        setIsRunning(false);
        setIsBreak(false);
        setMinutes(POMODORO_MINUTES);
        setSeconds(0);
    };
    const addTask = ()=>{
        if (newTaskText.trim()) {
            const newTask = {
                id: Date.now().toString(),
                text: newTaskText,
                completed: false,
                coinReward: COINS_PER_TASK
            };
            setUserData((prev)=>({
                    ...prev,
                    tasks: [
                        ...prev.tasks,
                        newTask
                    ]
                }));
            setNewTaskText('');
        }
    };
    const toggleTask = (id)=>{
        const task = userData.tasks.find((t)=>t.id === id);
        if (!task) return;
        const wasCompleted = task.completed;
        const newXP = wasCompleted ? userData.xp - XP_PER_TASK : userData.xp + XP_PER_TASK;
        const newRank = calculateRank(newXP);
        const newLevel = calculateLevel(newXP);
        setUserData((prev)=>({
                ...prev,
                tasks: prev.tasks.map((t)=>t.id === id ? {
                        ...t,
                        completed: !t.completed
                    } : t),
                coins: wasCompleted ? prev.coins - COINS_PER_TASK : prev.coins + COINS_PER_TASK,
                totalCoins: wasCompleted ? prev.totalCoins : prev.totalCoins + COINS_PER_TASK,
                xp: newXP,
                level: newLevel,
                rank: newRank.name,
                rankIcon: newRank.icon,
                tasksCompleted: wasCompleted ? prev.tasksCompleted - 1 : prev.tasksCompleted + 1
            }));
        if (!wasCompleted) {
            showRewardNotification(`Task Complete! +${COINS_PER_TASK} coins, +${XP_PER_TASK} XP`);
        }
    };
    const deleteTask = (id)=>{
        setUserData((prev)=>({
                ...prev,
                tasks: prev.tasks.filter((t)=>t.id !== id)
            }));
    };
    const redeemPlaytime = (minutes)=>{
        const cost = minutes * PLAYTIME_COST_PER_MINUTE;
        if (userData.coins >= cost) {
            setUserData((prev)=>({
                    ...prev,
                    coins: prev.coins - cost,
                    playtimeMinutes: prev.playtimeMinutes + minutes
                }));
            showRewardNotification(`Redeemed ${minutes} minutes of playtime!`);
        }
    };
    const exportData = ()=>{
        const dataStr = JSON.stringify(userData, null, 2);
        const dataUri = 'data:application/json;charset=utf-8,' + encodeURIComponent(dataStr);
        const exportFileDefaultName = `productivity_data_${Date.now()}.json`;
        const linkElement = document.createElement('a');
        linkElement.setAttribute('href', dataUri);
        linkElement.setAttribute('download', exportFileDefaultName);
        linkElement.click();
    };
    const importData = (event)=>{
        const file = event.target.files?.[0];
        if (file) {
            const reader = new FileReader();
            reader.onload = (e)=>{
                try {
                    const data = JSON.parse(e.target?.result);
                    setUserData(data);
                    showRewardNotification('Data imported successfully!');
                } catch (error) {
                    showRewardNotification('Error importing data');
                }
            };
            reader.readAsText(file);
        }
    };
    const currentRank = calculateRank(userData.xp);
    const nextRank = RANKS[RANKS.indexOf(currentRank) + 1] || currentRank;
    const xpProgress = (userData.xp - currentRank.minXP) / (nextRank.minXP - currentRank.minXP) * 100;
